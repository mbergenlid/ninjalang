
class ArrayList {
    private var array: Array = Array.ofSize(10);

    var size: Int = 0
        private set;
    var capacity: Int
        get = array.size;

    def get(i: Int): Any = array[i];
    def set(i: Int, value: Any): Unit = array[i] = value;

    def add(x: Any): Unit = {
        array[size] = x;
        size = size + 1;
    }

    //private def ensureCapacity(newCapacity: Int): Unit = {
    //    if(newCapacity > capacity) {
    //        val newArray = Array.ofSize(newCapacity);
    //    }
    //}

}
