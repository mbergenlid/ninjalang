
class ArrayList {
    private var array: Array = Array.ofSize(10);

    var size: Int = 0
        private set;
    var capacity: Int
        get = array.size;

    def get(i: Int): Any = array[i];
    def set(i: Int, value: Any): Nothing = array[i] = value;

    def add(x: Any): Nothing = {
        array[size] = x;
        size = size + 1;
    }


    private def ensureCapacity(newCapacity: Int): Nothing = {
        if(newCapacity > capacity) {
        }
    }

}